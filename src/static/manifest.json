{
    "manifest_version": 3,
    "name": "Webpack Boilerplate",
    "description": "Webpack Boilerplate",
    "version": "1.0.0",
    "permissions": [
        "tabs",
        "bookmarks",
        "storage",
        "identity"
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ]
}